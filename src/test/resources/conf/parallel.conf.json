{
  "server": "hub.browserstack.com",
  "user": "BROWSERSTACK_USERNAME",
  "key": "BROWSERSTACK_ACCESS_KEY",

  "capabilities": {
    "bstack:options": {
      "buildName": "browserstack-build-1",
      "sessionName": "BStack parallel testng",
      "browserVersion": "latest",
      "debug": true,
      "local": true
    }
  },

  "environments": {
    "env1": {
      "browserName": "chrome"
    },
    "env2": {
      "browserName": "firefox"
    },
    "env3": {
      "bstack:options": {
        "os": "OS X"
      },
      "browserName": "safari"
    }
  }
}